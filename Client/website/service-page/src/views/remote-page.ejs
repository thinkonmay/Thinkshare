<!DOCTYPE html>
<html>

<head>
  <link href="remote-page-asset/material-floating-button/dist/mfb.css" rel="stylesheet">
  <link href="remote-page-asset/css/googleapi.css" rel="stylesheet" />
  <link href="remote-page-asset/css/vuetify.css" rel="stylesheet" />
  <link href="remote-page-asset/css/dev.css" rel="stylesheet" />
  <link href="remote-page-asset/css/loading-icon.css" rel="stylesheet" />





  <title>Thinkmay | Remote control</title>
  <% include partials/head %>
</head>

<body style="overflow: hidden;">
  <div id="app">
    <v-app style="position: relative;">

          <ul class="mfb-component--br mfb-slidein" data-mfb-toggle="hover">
            <li class="mfb-component__wrap">
                    <!-- the main menu button -->
                    <a data-mfb-label=""  class="mfb-component__button--main">
                    <!-- the main button icon visibile by default -->
                        <i class="mfb-component__main-icon--resting ion-social-github">                                
                          <v-btn icon>
                            <v-icon x-large>mdi-plus</v-icon>
                        </v-btn>
                        </i>
                        <i data-mfb-label="Setting"  class="mfb-component__main-icon--active ion-social-github"> 
                          <v-btn icon id="GetSetting">
                            <v-icon x-large>mdi-plus</v-icon>
                        </v-btn>
                        </i>
                    </a>
                <ul class="mfb-component__list">
                    <li>
                      <a data-mfb-label="Go back to service page" class="mfb-component__button--child">
                          <i class="mfb-component__child-icon ion-social-twitter">                            
                              <v-btn icon href="/dashboard">
                                  <v-icon x-large>home</v-icon>
                              </v-btn>
                          </i>
                      </a>
                    </li>
                    <li>
                        <a data-mfb-label="Full screen mode" class="mfb-component__button--child">
                            <i class="mfb-component__child-icon ion-social-twitter">                            
                                <v-btn icon v-on:click="enterFullscreen()">
                                    <v-icon x-large>fullscreen</v-icon>
                                </v-btn>
                            </i>
                        </a>
                    </li>
                </ul>
            </li>
        </ul>


      <div id="video_container" class="video-container">
        <video id="stream" autoplay playsinline> -->
          Your browser doesn't support video
        </video>
      </div>
      <div id="loading" style="position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); background-color: red;">
        <section class="wrapper dark">
          <div class="spinner">
            <i></i>
            <i></i>
            <i></i>
            <i></i>
            <i></i>
            <i></i>
            <i></i>
          </div>
        </section>
      </div>

    </v-app>
  </div>
</body>

<script src="https://webrtc.github.io/adapter/adapter-latest.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.6.9/vue.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/vuetify/1.5.14/vuetify.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/vue-spinner@1.0.3/dist/vue-spinner.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.2/axios.min.js"></script>

<script src="remote-page-asset/src/app.js?ts=1"></script>
<script src="remote-page-asset/src/enum.js?ts=1"></script>
<script src="remote-page-asset/src/quality-track.js?ts=1"></script>
<script src="remote-page-asset/src/input.js?ts=1"></script>
<script src="remote-page-asset/src/signalling.js?ts=1"></script>
<script src="remote-page-asset/src/webrtc.js?ts=1"></script>
<script src="remote-page-asset/src/client-message.js?ts=1"></script>
<script src="remote-page-asset/src/clipboard.js?ts=1"></script>

<script type="module" src="javascripts/pages/remote-page.js"></script>

</html>